{
	"manifest_version": 3,
	"name": "Recent Tabs",
	"version": "0.0.1",

	"author": "Tony",
	"description": "",

	"permissions": [
		"storage"
	],
	"commands": {
		"toggle-tab": {
			"suggested_key": {
				"default": "Ctrl+Q"
			},
			"description": "Key to toggle between last two recently used tabs"
		}
	},
	"background": {
		"service_worker": "background.js"
	}
}
